alias info='info --vi-keys'

# If not running interactively, exit
[[ $- != *i* ]] && return

# Prevent accidentally closing shells when I press CTRL+d to move the screen down
set -o ignoreeof

alias e="emacsclient -n"
alias et="emacsclient -t"
alias v="view_in_emacs"
alias here="pwd | xclip"
# alias dc="docker compose"
alias mc="mutagen-compose"
alias msh="mosh --server='/home/john/.nix-profile/bin/mosh-server'"
alias k="kubectl"
alias rgf="rg --files"

export EDITOR='emacsclient -t'
export CRYPTOGRAPHY_OPENSSL_NO_LEGACY=1 # Tell conda to use the new openssl
